num = int(input("Enter a number >0: "))

def prime(num):
    
    for i in list(range(2,num)):
        if num % i == 0:
            return True
        else:
            return False
if prime(num) == True:
    print("This is not a prime number!")
elif num < 2:
    print("This is not a prime number!")
elif num == 2:
    print("This is the only even prime number")
else:
    print("This is a prime number!")
