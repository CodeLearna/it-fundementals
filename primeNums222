number = int(input("Please enter a positive number : "))
primelist = []

print("Prime numbers between", 0,"and",number, "are:")

for i in range(number+ 1):
  
   if i > 1:
   
       for k in range(2, i):
           if (i % k) == 0:
               break
       else:
           primelist.append(i)

print(primelist)
